{
    "name": "My Codespace with Postgres",
    "dockerComposeFile": [
        "docker-compose.yaml"
    ],
    "service": "devcontainer",
    "workspaceFolder": "/workspace",
    "forwardPorts": [
        5432
    ],
    "features": { "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {} },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-azuretools.vscode-docker",
                "ms-python.python"
            ]
        }
    }
}